
#include <iostream>
using namespace std;

int main()
{
    int a, x, y;
    cin >> a >> x >> y;
    char array[100][100]{};

    for (int i = 0; i < a; i++)
    {
        for (int j = 0; j < a; j++)
        {
            array[i][j] = '*';
        }
    }
    for (int i = 0; i < a; i++)
    {
        for (int c = 0; c < y && i == 0; c++) cout << endl;
        for (int j = 0; j < a; j++)
        {
            for (int cc = 0; cc < x && j == 0; cc++) cout << ' ';
            cout << array[i][j];
        }
        cout << endl;
    }
    system("pause");
}
